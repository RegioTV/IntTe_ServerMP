<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">

	<converter>
		<converter-for-class>ch.hsr.intte.servermp.model.Room</converter-for-class>
		<converter-class>ch.hsr.intte.servermp.model.RoomConverter</converter-class>
	</converter>

	<application>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>de</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>ch.hsr.intte.servermp.Messages</base-name>
			<var>messages</var>
		</resource-bundle>
	</application>
</faces-config>
